{
    "manifest_version": 3,
    "name": "Start New Tab",
    "version": "1.0",
    "description": "Configura Start como tu página de inicio, de arranque y de nueva pestaña.",
    "permissions": [
      "storage",
      "tabs"
    ],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "128": "icon128.png"
      },
      "default_title": "Start New Tab"
    },
    "icons": {
      "128": "icon128.png"
    }
  }